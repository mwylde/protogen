tcp_header = {
  public source_port: u16;
  public destination_port: u16;
  public sequence_number: u32;
  public acknowledgement_number: u32;
  @data_offset: u4;
  public reserved: u3;
  public ns: u1;
  public cwr: u1;
  public ece: u1;
  public urg: u1;
  public ack: u1;
  public psh: u1;
  public rst: u1;
  public syn: u1;
  public fin: u1;
  public window_size: u16;
  public checksum: u16;
  // TODO: parse these
  public options: [u8; (@data_offset - 5) * 4];
}

